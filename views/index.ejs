<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="stylesheet" href="/assets/css/index.css" />
    <link rel="stylesheet" href="/assets/css/all.min.css" />
    <link rel="stylesheet" href="/assets/css/fonts.css" />
    <script src="/assets/scripts/script.js"></script>
    <title>HooHoop</title>
    <link
      rel="icon"
      href="/assets/images/favicon.ico"
      type="image/ico"
      sizes="16*16"
    />
  </head>

  <body>
    <%-include('./Components/Header.ejs') %>
    <!--Navbar-end-->
    
    <!--Slider-->
    <div class="slider">
      <img src="/assets/images/Slider.jpg">
      <div class="swrapfm">
        <div class="buy-car">
          <div class="card_headline">Need a Car ?</div>
          <form method="GET" class="data_fetch">
            <div class="card_input_info">

              <input type="text" name="car" value="true">
              
              <div class="vehicle_budget input_div">
                <select name="Price" id="v-budget">
                  <option value="" selected>Select Budget</option>
                  <option value="0-5000">0 - $5000</option>
                  <option value="0-10000">$5000 - 10000</option>
                  <option value="10000-9999999">$10000+</option>
                </select>
              </div>

              <div class="vehicle_info_wrap input_div">
                <select name="BodyType" id="v-type">
                  <option value="" selected>Select Vehicle Type</option>
                  <option value="Convertible">Convertible</option>
                  <option value="Hatchback">Hatchback</option>
                  <option value="Sedan">Sedan</option>
                  <option value="Station Wagon">Station Wagon</option>
                  <option value="Utility">Utility</option>
                </select>
              </div>

              <div class="vehicle_state input_div">
                <select name="fuelType" id="v-state">
                  <option value="" selected>Select Fuel Type</option>
                  <option value="Petrol">Petrol</option>
                  <option value="Diesel">Diesel</option>
                  <option value="Electric">Electric</option>
                  <option value="Hybrid">Hybrid</option>
                </select>
              </div>

            </div>
            <div class="submit_input_info">
              <button class="submit_data" type="submit" formaction="/search-car/1">submit</button>
            </div>
          </form>
        </div>
      </div>
    </div>
    <!--Slider-end-->

    <!--Home-content-->

    <div class="infowrap80">
      <div class="cardbd_1">
        <div class="cd_head">
          <div class="cd_headline">Popular Cars</div>
        </div>
        <div class="cd_content">
        <% for( let i = 0; i < popCarData.length; i++ ) { %>
          <a href="/buy-car/<%= popCarData[i]._id %>" class="cd_car_data">
            
              <div class="img_contain">
                <div class="view-details">View details</div>
                <img src="/assets/Uploads/<%= popCarData[i].vinNum %>/exterior/Photo_1.jpg" />
              </div>
              <div class="info_contain">
                <div class="con_name"> <%= popCarData[i].Make %> </div>
                <div class="con_price"> $ <%= popCarData[i].Price %></div>
                <div></div>
              </div>
            
          </a>
        <% } %>
      </div>
        <div class="cd_btn">
          <button onclick="scrolldiv(0,0)">
            <i class="fal fa-chevron-left"></i>
          </button>
          <button onclick="scrolldiv(1,0)">
            <i class="fal fa-chevron-right"></i>
          </button>
        </div>
        <div class="cd_link">
          <a href="/search-car/1">View All</a>
        </div>
      </div>
  
