            </div>
          </div>
          <div id="age" class="filter_option">
            <div class="Filter_class"><span>AGE</span><i class="fas fa-chevron-right"></i></div>
          </div>
          <div id="transmission" class="filter_option">
            <div class="Filter_class"><span>Transmission</span><i class="fas fa-chevron-right"></i></div>
          </div>
          <div id="body_type" class="filter_option">
            <div class="Filter_class"><span>Body Type</span><i class="fas fa-chevron-right"></i></div>
          </div>
          <div id="color" class="filter_option">
            <div class="Filter_class"><span>Color</span><i class="fas fa-chevron-right"></i></div>
          </div>
        </form>
        </div>
      </div>
  
      <div class="contentwrap">
        <% for(var i = 0; i < record.length; i++) { %>
          <a href="http://localhost:8080/buy-car/<%= record[i].id%>" class="cardanchor">
          <div class="bc-card">
            <div class="cb_img"><img src="/assets/Uploads/<%= record[i].vinNum %>/exterior/<%= record[i].thumbnail %>"></div>
            <div class="cb_info">
              <div class="cb_main">
                <span class="cb_name"><%= record[i].Make %></span>
                <span class="cb_price">$ <%= record[i].Price %></span>
              </div>
              <div class="cb_add_info">
                <span class="kms_info"><%= record[i].kMeters %></span>
                <span class="trans_info"><%= record[i].Transmission %></span>
                <span class="model_info"><%= record[i].Model %></span>
              </div>
            </div>
          </div>
        </a>
        <% } %>
