              <div class="foption">V60 <input type="checkbox" id="foption-67"> <label for="foption-67"></label></div>
              <div class="foption">VITZ <input type="checkbox" id="foption-68"> <label for="foption-68"></label></div>
              <div class="foption">Wish <input type="checkbox" id="foption-69"> <label for="foption-69"></label></div>
              <div class="foption">XC60 <input type="checkbox" id="foption-70"> <label for="foption-70"></label></div>
              <div class="foption">XJ <input type="checkbox" id="foption-71"> <label for="foption-71"></label></div>
              <div class="foption">Z3 <input type="checkbox" id="foption-72"> <label for="foption-72"></label></div>
            </div>
          </div>
          <div id="price" class="filter_option">
            <div class="Filter_class" onclick="foption('2')"><span>Price</span><i class="fas fa-chevron-right"></i></div>
            <div class="foption_encloser">
              <div class="foption">$ 0 To $ 3000 <input type="checkbox" id="foption-73"> <label for="foption-73"></label></div>
              <div class="foption">$ 3001 To $ 5000 <input type="checkbox" id="foption-74"> <label for="foption-74"></label></div>
              <div class="foption">$ 5001 To $ 8000 <input type="checkbox" id="foption-75"> <label for="foption-75"></label></div>
